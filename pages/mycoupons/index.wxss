.container {
    background-color:#F2f2f2;
}
.coupons-list{
    width: 100%;
    background-color: #fff;
    margin-top: 20rpx;
}

.bottom-box{
    position:fixed;
    top: 20rpx;
    left: 0;
    font-size:28rpx;
    color:#000000;
    height:100%;
    line-height: 100rpx;
    background: #fff url(../../images/arrow-down.png) no-repeat 30rpx;
}
.bottom-box .add-btn{
    margin-top: 30rpx;
    margin-left: 30rpx;
    padding-top: 70rpx;
    background: url(https://cdn.it120.cc/images/weappshop/add-addr.png) no-repeat left center;
    background-size: 40rpx auto;
}
.coupons-item{
  width:630rpx;
  height: 180rpx;
  margin: 10rpx;
  padding: 50rpx;
  background-color: #f1a83b;
  font-size: 30rpx;
  line-height: 50rpx;
  color: #fff;
}